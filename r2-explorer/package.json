{
  "name": "r2-explorer",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "wrangler dev",
    "dev:web": "pnpm -C web dev",
    "build:web": "pnpm -C web build",
    "check:api": "tsc --noEmit",
    "check:web": "pnpm -C web check",
    "check": "pnpm run check:api && pnpm run check:web",
    "deploy": "wrangler deploy",
    "test:api": "vitest run",
    "test": "pnpm run test:api",
    "test:live": "vitest run tests/live.integration.spec.ts",
    "test:watch": "vitest"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20260305.0",
    "hono": "^4.12.3",
    "miniflare": "^4.20260305.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18",
    "wrangler": "^4.69.0"
  },
  "dependencies": {
    "aws4fetch": "^1.0.20",
    "zod": "^4.3.6"
  },
  "pnpm": {
    "overrides": {
      "wrangler": "^4.69.0"
    }
  }
}
